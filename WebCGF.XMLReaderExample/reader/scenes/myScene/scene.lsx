	    <NODE id="plate">
	   		<MATERIAL id="grey" />
	    	<TEXTURE id="null" />

	        <TRANSLATION x="0" y="0" z="1"/>
	        <ROTATION axis="x" angle="-90"/>

	    	<DESCENDANTS>
	        	<DESCENDANT id="rect"/>
	        </DESCENDANTS>
	   	</NODE>

	   	<NODE id="player2">
	   		<MATERIAL id="green" />
	    	<TEXTURE id="null" />


	    	<DESCENDANTS>
	        	<DESCENDANT id="game_piece"/>
	        </DESCENDANTS>
	   	</NODE>

	   	<NODE id="player1">
	   		<MATERIAL id="yellow" />
	    	<TEXTURE id="null" />

	    	<DESCENDANTS>
	        	<DESCENDANT id="game_piece"/>
	        </DESCENDANTS>
	   	</NODE>

	   	<NODE id="game_piece">
	   		<MATERIAL id="null" />
	    	<TEXTURE id="null" />

	    	<DESCENDANTS>
	        	<DESCENDANT id="base_game_piece"/>
	        	<DESCENDANT id="base_game_piece_1"/>
	        	<DESCENDANT id="side_game_piece"/>
	        	<DESCENDANT id="side_game_piece_1"/>
	        	<DESCENDANT id="side_game_piece_2"/>
	        </DESCENDANTS>
	   	</NODE>

	   	<NODE id="base_game_piece">
	   		<MATERIAL id="null" />
	    	<TEXTURE id="null" />

	    	<ROTATION axis="z" angle="180"/>
	    	<TRANSLATION x="-0.5" y="-0.2" z="0"/>

	    	<DESCENDANTS>
	        	<DESCENDANT id="tri"/>
	        </DESCENDANTS>
	   	</NODE>

