	    	<!--<SCALE sx="1" sy="0.5" sz="0"/>-->
	    	<TRANSLATION x="0" y="0" z="1"/>
	    	<ROTATION axis="y" angle="63.6"/>
	    	<SCALE sx="1.115" sy="0.2" sz="0"/>


	    	<DESCENDANTS>
	        	<DESCENDANT id="rect"/>
	        </DESCENDANTS>
	   	</NODE>

